include ./includes/header.pug

//-------- 객실달력 Page ---------//
body(name='calendar')
  include ./includes/main-nav-left.pug
  include ./includes/sub-nav.pug
  .content-container
    .artboard
      .row
        .col-lg.calendar-controler
          .form-inline
            .year-month 2018년 1월
            .btn-group.mx-3
              button.btn.btn-light(type='button')
                i.fa.fa-angle-left.fa-lg
              button.btn.btn-light(type='button')
                i.fa.fa-angle-right.fa-lg
            button.btn.btn-light.active.mr-2(type='button')
              | month
            button.btn.btn-light(type='button')
              | week

        .col-lg.status-model.d-flex.justify-content-end
          .status-badge.available
          | 예약가능
          .status-badge.waiting
          | 예약대기
          .status-badge.complete
          | 예약완료
          .status-badge.callonly
          | 전화문의
          .status-badge.blocked
          | 예약불가

